<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint revision="1" name="default">
  <PreFlow name="PreFlow">
    <Request>
      <Step>
        <Name>FC-Authentication</Name>
      </Step>
    </Request>
  </PreFlow>
  <PostFlow name="PostFlow">
    <Request>
      <Step>
        <Name>AM-UserIdHeader</Name>
        <Condition>accesstoken.climateiq_user_id != null</Condition>
      </Step>
      <Step>
        <Name>FC-CORS</Name>
      </Step>
    </Request>
  </PostFlow>
  <DefaultFaultRule name="default">
    <Step>
      <Name>FC-CORS</Name>
    </Step>
  </DefaultFaultRule>
  <HTTPProxyConnection>
    <BasePath>/data</BasePath>
    <!-- Listen only on the HTTPS endpoint -->
    <VirtualHost>secure</VirtualHost>
  </HTTPProxyConnection>
  <RouteRule name="NoRoute"/>
</ProxyEndpoint>
