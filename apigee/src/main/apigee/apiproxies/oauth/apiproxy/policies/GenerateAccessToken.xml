<OAuthV2 name="GenerateAccessToken">
  <Operation>GenerateAccessToken</Operation>
  <!-- Expire in 3600000 msec or 1 hour -->
  <ExpiresIn>3600000</ExpiresIn>
  <SupportedGrantTypes>
    <GrantType>client_credentials</GrantType>
  </SupportedGrantTypes>
  <GrantType>request.queryparam.grant_type</GrantType>
   <Attributes>
    <!-- TODO: Retrieve UUID from Firebase ID token -->
    <Attribute name='firebase_uuid' display='true'>12345</Attribute>
  </Attributes>
  <GenerateResponse/>
</OAuthV2>