<CORS async="false" continueOnError="false" enabled="true" name="CORS-AddHeaders">
  <AllowOrigins>https://auth.climateiq.org</AllowOrigins>
  <AllowMethods>POST, DELETE</AllowMethods>
  <AllowHeaders>origin, x-requested-with, accept, content-type, authorization</AllowHeaders>
  <ExposeHeaders>*</ExposeHeaders>
  <AllowCredentials>true</AllowCredentials>
  <GeneratePreflightResponse>true</GeneratePreflightResponse>
</CORS>
